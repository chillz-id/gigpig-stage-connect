{
  "installCommand": "npm install --legacy-peer-deps",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "feedback.standupsydney.com"
        }
      ],
      "destination": "https://docs.google.com/forms/d/e/1FAIpQLSe9zUS8YC29JZZwFmJj1Qu8UJjJHvgD5AA54sF1_MEbvTTiXg/viewform?usp=header",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/(.*)", "destination": "/" }
  ]
}